        [36mtarget[0m=[38;5;130m"[0m[35m${smb_root[0m[38;5;130m%[0m/[35m}[0m[31m/[0m[35m${share_subdir}[0m[38;5;130m"[0m
        info [38;5;130m"[0m[31mCreating target and symlink inside CT [0m[35m$ctid[0m[31m: [0m[35m$app_path[0m[31m -> [0m[35m$target[0m[38;5;130m"[0m[11;9Hpct [38;5;130mexec[0m [38;5;130m"[0m[35m$ctid[0m[38;5;130m"[0m [38;5;130m--[0m bash [38;5;130m-c[0m [38;5;130m"[0m[31mset -e; mkdir -p [0m[35m\"[0m[35m${target}[0m[35m\"[0m[31m; mkdir -p [0m[35m\"[0m[35m$([0m[35mdirname \"[0m[35m$app_path[0m[35m\"[0m[35m)[0m[35m\"[0m[31m; if [ -L [0m[35m\"[0m[35m${app_path}[0m[35m\"[0m[31m ]  [12;1H|| [ -e [0m[35m\"[0m[35m${app_path}[0m[35m\"[0m[31m ]; then rm -rf [0m[35m\"[0m[35m${app_path}[0m[35m\"[0m[31m; fi; ln -s [0m[35m\"[0m[35m${target}[0m[35m\"[0m[31m [0m[35m\"[0m[35m${app_path}[0m[35m\"[0m[38;5;130m"[0m
    [38;5;130mdone[0m
[36m}[0m[15;1H[K[16;1H[7m[36msetu[0m[36mp_generic_container() {[0m[16;28H[K[17;5H[38;5;130mlocal[0m [36mname[0m=[35m$1[0m[17;18H[K[18;5H[38;5;130mlocal[0m [36murl[0m=[35m${2[0m[38;5;130m:-[0m[35m}[0m
    [38;5;130mshift[0m [31m2[0m [38;5;130m||[0m [38;5;130mtrue
[0m    [38;5;130mlocal[0m [36mmounts[0m=[35m()[0m
    [38;5;130mlocal[0m [36mexit_node[0m=[21;21H[K[22;1H    [38;5;130mlocal[0m [36msmb_host[0m=[38;5;130m""[0m[22;22H[K[23;1H    [38;5;130mlocal[0m [36msmb_share[0m=[38;5;130m"[0m[31mstorage[0m[38;5;130m"[0m[23;30H[K[24;1H    [38;5;130mlocal[0m [36msmb_mountpoint[0m=[38;5;130m"[0m[31m/mnt/storage[0m[38;5;130m"[0m[24;40H[K[25;5H[38;5;130mlocal[0m [36msymlinks[0m=[35m()[0m[26;1H[K[27;1H    [38;5;130mwhile [[0m [38;5;130m"[0m[35m$#[0m[38;5;130m"[0m [38;5;130m-gt[0m [31m0[0m [38;5;130m]; do[28;9Hcase[0m [38;5;130m"[0m[35m$1[0m[38;5;130m"[0m [38;5;130min[0m
            --mount[38;5;130m)[0m
                [36mmounts[0m+=[35m([0m[38;5;130m"[0m[35m$2[0m[38;5;130m"[0m[35m)[0m[38;5;130m;[0m [38;5;130mshift[0m [31m2[0m[38;5;130m;;[0m
            --exit-node[38;5;130m)[0m[31;25H[K[32;117H163,1[9C68%[32;117H[K[32;6H[34h[?25hp[?25l[16;5H[7m[36mp[0m[32;117H163,1[9C68%[32;117H[K[32;7H[34h[?25h_[?25l[16;6H[7m[36m_[0m[32;117H163,1[9C68%[32;117H[K[32;8H[34h[?25hg[?25l[16;7H[7m[36mg[0m[32;117H163,1[9C68%[32;117H[K[32;9H[34h[?25he[?25l[16;8H[7m[36me[0m[32;117H163,1[9C68%[32;117H[K[32;10H[34h[?25hn[?25l[16;9H[7m[36mn[0m[32;117H163,1[9C68%[32;117H[K[32;11H[34h[?25he[?25l[16;10H[7m[36me[0m[32;117H163,1[9C68%[32;117H[K[32;12H[34h[?25hr[?25l[16;11H[7m[36mr[0m[32;117H163,1[9C68%[32;117H[K[32;13H[34h[?25hi[?25l[16;12H[7m[36mi[0m[32;117H163,1[9C68%[32;117H[K[32;14H[34h[?25hc[?25l[16;13H[7m[36mc[0m[32;117H163,1[9C68%[32;117H[K[32;15H[34h[?25h[?1000h[?25l[16;1H[36msetup_generic[0m[32;117H163,1[9C68%[16;1H[34h[?25h[?25l[32;107H/[16;1H[32;1H[K[32;1H/[?1000l[34h[?25hn[?25l[16;9H[7m[36mn[0m[32;117H163,9[9C68%[32;117H[K[32;3H[34h[?25ha[?25l[16;9H[36mn[0m[17;11H[7m[36mna[0m[32;117H164,11[8C68%[32;117H[K[32;4H[34h[?25hm[?25l[17;13H[7m[36mm[0m[32;117H164,11[8C68%[32;117H[K[32;5H[34h[?25he[?25l[17;14H[7m[36me[0m[32;117H164,11[8C68%[32;117H[K[32;6H[34h[?25h[?1000h[?25l[17;11H[36mname[0m[32;117H164,11[8C68%[17;11H[34h[?25h[?25l[32;107Hn[17;11H[32;107H[K[32;1H[26;44H[1;31r[1;1H[21M[1;32r[11;17H[36mexit_node[0m=[38;5;130m"[0m[35m$2[0m[38;5;130m";[0m [38;5;130mshift[0m [31m2[0m[38;5;130m;;[0m[12;13H--smb-host[38;5;130m)[0m[13;17H[36msmb_host[0m=[38;5;130m"[0m[35m$2[0m[38;5;130m";[0m [38;5;130mshift[0m [31m2[0m[38;5;130m;;[0m[14;13H--smb-share[38;5;130m)[0m[15;17H[36msmb_share[0m=[38;5;130m"[0m[35m$2[0m[38;5;130m";[0m [38;5;130mshift[0m [31m2[0m[38;5;130m;;[0m[16;13H--smb-mountpoint[38;5;130m)[0m[17;17H[36msmb_mountpoint[0m=[38;5;130m"[0m[35m$2[0m[38;5;130m";[