[38;5;130mset[0m[36m [0m[35m-euo[0m[36m pipefail[0m[2;18H[K[3;1H[K[4;1H[34m# Common helpers for the install and container scripts[0m
[36minfo()  {[0m [38;5;130mprintf[0m [38;5;130m'[0m[31m%s | INFO  | %s\n[0m[38;5;130m'[0m [38;5;130m"[0m[35m$([0m[35mdate -Iseconds[0m[35m)[0m[38;5;130m"[0m [38;5;130m"[0m[35m$*[0m[38;5;130m";[0m [36m}
