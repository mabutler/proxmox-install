echo[0m[31m [0m[38;5;130m"[0m[31mqBittorrent container setup completed (CT may still require 'tailscale up' inside the CT).[0m[38;5;130m"[0m[32;117H9,0-1[9CAll[9;1H[34h[?25h[?25l[32;107Hj[9;1H[32;107H [10;1H[32;117H10,1 [10;1H[34h[?25h[?25l[32;107Hj[10;1H[32;107H [11;1H[32;118H1[11;1H[34h[?25h[?25l[32;107Hj[11;1H[32;107H [12;1H[32;118H2,0-1[12;1H[34h[?25h[?25l[32;107Hj[12;1H[32;107H [13;1H[32;118H3,1  [13;1H[34h[?25h[?25l[32;107Hj[13;1H[32;107H [14;1H[32;118H4[14;1H[34h[?25h[?25l[32;107Hj[14;1H[32;107H [15;1H[32;118H5[15;1H[34h[?25h[?25l[32;107Hj[15;1H[32;107H [16;1H[32;118H6[16;1H[34h[?25h[?25l[32;107Hj[16;1H[32;107H [17;1H[32;118H7,0-1[17;1H[34h[?25h[?25l[32;107Hj[17;1H[32;107H [18;1H[32;118H8,1  [18;1H[34h[?25h[?25l[32;107Hj[18;1H[32;107H [19;1H[32;118H9[19;1H[34h[?25h[?25l[32;107Hj[19;1H[32;107H [20;8H[32;117H20,1-8[20;8H[34h[?25h[?25l[32;107Hj[20;8H[32;107H [21;8H[32;118H1[21;8H[34h[?25h[?25l[32;107Hj[21;8H[32;107H [22;8H[32;118H2[22;8H[34h[?25h[?25l[32;107Hj[22;8H[32;107H [23;1H[19;10H[35m[106m([23;1H)[0m[32;118H3,1  [23;1H[34h[?25h[?25l[32;107Hj[23;1H[32;107H [24;1H[19;10H[35m([23;1H)[0m[32;118H4,0-1[24;1H[34h[?25h[?25l[32;107Hj[24;1H[32;107H [25;1H[32;118H5,1  [25;1H[34h[?25h[?25l[32;107Hj[25;1H[32;107H [26;8H[32;118H6,1-8[26;8H[34h[?25h[?25l[32;107Hj[26;8H[32;107H [27;8H[32;118H7[27;8H[34h[?25h[?25l[32;107Hj[27;8H[32;107H [28;8H[32;118H8[28;8H[34h[?25h[?25l[32;107Hj[28;8H[32;107H [29;1H[32;118H9,0-1[29;1H[34h[?25h[?25l[32;107Hj[29;1H[32;107H [30;1H[32;117H30,1  [30;1H[34h[?25h[?25l[32;107Hj[30;1H[32;107H [31;1H[32;118H1,0-1[31;1H[34h[?25h[?25l[32;107Hj[31;1H[32;107H [31;1H[34h[?25h[?25l[32;107Hj[31;1H[32;107H [31;1H[34h[?25h[?25l[32;107Hj[31;1H[32;107H [31;1H[34h[?25h[?25l[32;107Hk[31;1H[32;107H [30;1H[32;118H0,1