require_root() {[0m
    [38;5;130mif [[0m [38;5;130m"[0m[35m$([0m[35mid -u[0m[35m)[0m[38;5;130m"[0m [38;5;130m-ne[0m [31m0[0m [38;5;130m];[0m [38;5;130mthen[0m[11;9Hdie [38;5;130m"[0m[31mThis script must be run as root[0m[38;5;130m"
[0m    [38;5;130mfi[0m
[36m}

ensure_command() {[0m
    [38;5;130mif ![0m [38;5;130mcommand[0m [38;5;130m-v[0m [38;5;130m"[0m[35m$1[0m[38;5;130m"[0m [38;5;130m>[0m/dev/null [31m2[0m[38;5;130m>&[0m[31m1[0m[38;5;130m;[0m [38;5;130mthen[0m[17;9Hdie [38;5;130m"[0m[31mRequired command '[0m[35m$1[0m[31m' not found[0m[38;5;130m"
[0m    [38;5;130mfi[0m
[36m}[0m

[34m# Repo layout helpers
# REPO_ROOT is the repository root (scripts/ is expected to be under it)[0m
[36mREPO_ROOT[0m=[38;5;130m"[0m[35m$([0m[38;5;130mcd[0m[35m [0m[38;5;130m"[0m[35m$([0m[35mdirname [0m[38;5;130m"[0m[35m$0[0m[38;5;130m"[0m[35m)[0m[31m/..[0m[38;5;130m"[0m[35m [0m[38;5;130m&&[0m[35m [0m[38;5;130mpwd[0m[35m)[0m[38;5;130m"[0m
[36mTOOLS_DIR[0m=[38;5;130m"[0m[35m${TOOLS_DIR[0m[38;5;130m:-[0m[35m$REPO_ROOT[0m/tools[35m}[0m[38;5;130m"[0m

[34m# Fail fast if Proxmox LXC tooling isn't available where we expect it to be.[0m
ensure_command pct

[36mget_ctid_by_name() {[0m
    [38;5;130mlocal[0m [36mname[0m=[35m$1[0m
    [34m# Match the container name (last column) case-insensitively to avoid[0m[32;117H1,1[11CTop[1;1H[34h[?25h[?25l[32;107H/[1;1H[32;1H[K[32;1H/[?1000l[34h[?25hs[?25l[1;5H[7m[34ms[0m[32;117H1,5[11CTop[32;117H[K[32;3H[34h[?25he[?25l[1;5H[34ms[0m
