[0m    [38;5;130mdone[24;5Hlocal[0m ctid
    [38;5;130mif [[0m [38;5;130m-n[0m [38;5;130m"[0m[35m$url[0m[38;5;130m"[0m [38;5;130m];[0m [38;5;130mthen[0m[26;9H[36mctid[0m=[35m$([0m[35mensure_container_from_url [0m[38;5;130m"[0m[35m$name[0m[38;5;130m"[0m[35m [0m[38;5;130m"[0m[35m$url[0m[38;5;130m"[0m[35m)[0m
